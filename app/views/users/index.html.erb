
<%= render 'books/user_info', book: @newbook %>

<h2>Users</h2>

<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><!--画像--></td>
        <td><%= user.name %></td>
        <td><%= link_to "show", user_path(user.id) %></td><!--仮置きユーザーの詳細画面へ-->
      </tr>
    <% end %>
  </tbody>
</table>